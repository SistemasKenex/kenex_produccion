<odoo>
    <data>
        <record model="ir.ui.view" id="product_template_view_inherit_1">
            <field name="name">product.template.view.inherit</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='inventory']" position="after">
                        <page name="kenex.product.template" string="(Internos) " >
                            <group>
                                <group>
                                    <field name="it_codigo_alterno"/> 
                                    <field name="it_descripcion_alterna_uno"/> 
                                    <field name="it_descripcion_alterna_dos"/> 
                                    <field name="it_descripcion_alterna_tres"/> 
                                    <field name="it_descripcion_alterna_cuatro"/> 
                                    <field name="it_use_hs_code"/> 
                                    <field name="it_hs_code_custom"/> 
                                    <field name="it_caja"/> 
                                    <field name="it_check_quantity"/>  
                                    <field name="it_block_quantity"/>  
                                </group>
                                <group>   
                                    <field name="it_bultos"/> 
                                    <field name="it_peso_bultos"/> 
                                    <field name="it_descontinuado"/> 
                                    <field name="it_product_type"/> 
                                    <field name="it_height"/> 
                                    <field name="it_long"/> 
                                    <field name="it_width"/> 
                                    <field name="it_ind_serial"/> 
                                    <field name="it_brand_id"/> 
                                </group>
                            </group>  
                        </page>
                </xpath>              
            </field>
        </record>	
    </data>
</odoo>

    


