<odoo>
    <data>
        <record id="it_res_company_form_view" model="ir.ui.view">
            <field name="name">res.company.form</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"></field>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//form/sheet/notebook/page[@name='general_info']" position="after">
                        <page string="Campos Propios IT">
                            <group name="it_new_fields">
                                <group name="it_new_fields_2" string="Campos Propios IT">
                                    <field name="it_company_id"/>
                                </group>
                                <group name="inactive_options">
                                </group>
                            </group>
                        </page>
                    </xpath>
                </data>
            </field>
        </record>
    </data>
</odoo>



